<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <appender name="A1" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%d [%t] %-5p %c - %m%n</pattern>
        </encoder>
    </appender>
    <root level="INFO">
        <appender-ref ref="A1"/>
    </root>
</configuration>